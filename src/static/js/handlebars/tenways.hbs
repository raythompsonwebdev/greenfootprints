

  <section class="content">
    <h1>10 Ways to recycle</h1>
    <section id="contentboxes">

      <div id="mask">
        {{#each tenways}}
        <article class="contentbox fade">
          <h1>{{this.title}}</h1>
          <p>{{this.text}}</p>
          <img id="tenImageTwo" src="{{this.image}}" alt="{{this.alt}}" />
        </article>
        {{/each}}


        <a class="prev" onclick="plusSlidesOne(-1)">&#10094;</a>
        <a class="next" onclick="plusSlidesOne(1)">&#10095;</a>

      </div>


    </section>
  </section>



