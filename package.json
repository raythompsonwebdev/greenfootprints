{
  "name": "greenfootprints",
  "version": "1.0.0",
  "description": "JavaScript development environment Pluralsight course by Cory House",
  "main": "index.js",
  "type": "module",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": " npm-run-all --parallel open:src",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "eslint webpack.config.* src --color --fix",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "retire --package",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist ",
    "build": "webpack --config webpack.config.prod.js --progress",
    "postbuild": "babel-node buildScripts/distServer.js",
    "deploy": "surge ./dist"
  },
  "author": "Raymond Thompson",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/node": "^7.22.19",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/runtime": "^7.23.8",
    "@squoosh/lib": "^0.5.3",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^9.1.3",
    "babel-preset-latest": "^6.24.1",
    "chai": "^5.0.0",
    "chalk": "5.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "compression": "^1.7.4",
    "css-loader": "^6.9.1",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-utils": "^3.0.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "html-loader": "^5.0.0",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^5.6.0",
    "image-minimizer-webpack-plugin": "^4.0.0",
    "image-webpack-loader": "^8.1.0",
    "mini-css-extract-plugin": "^2.7.7",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^8.0.0",
    "sass-loader": "^14.0.0",
    "style-loader": "^3.3.4",
    "stylelint": "^16.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-prettier": "^5.0.0",
    "stylelint-scss": "^6.0.0",
    "stylelint-webpack-plugin": "^4.1.1",
    "terser-webpack-plugin": "^5.3.10",
    "url-loader": "^4.1.1"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/register": "^7.23.7",
    "connect-history-api-fallback": "^2.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "open": "^8.4.0",
    "postcss": "^8.4.31",
    "postcss-preset-env": "^7.8.2",
    "postcss-scss": "^4.0.5",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.21.3",
    "sass": "^1.70.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.0.0",
    "webpack-hot-middleware": "2.26.0",
    "webpack-merge": "^5.10.0",
    "whatwg-fetch": "^3.6.20"
  },
  "repository": "git@github.com:raythompsonwebdev/greenfootprints.git"
}
